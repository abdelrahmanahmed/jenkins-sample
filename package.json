{
	"name": "jenkines",
	"version": "0.0.0",
	"devDependencies": {
		"commitizen": "^4.0.3",
		"cz-conventional-changelog": "^3.0.2",
		"semantic-release": "^15.13.21"
	},
	"scripts": {
    "semantic-release": "semantic-release",
    "commit": "git-cz"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/abdelrahmanahmed/jenkins-sample.git"
	},
	"release": {
		"branches": [
			"master"
		],
		"repositoryUrl": "https://github.com/abdelrahmanahmed/jenkins-sample.git",
		"plugins": [
			"@semantic-release/commit-analyzer",
			"@semantic-release/release-notes-generator",
			"@semantic-release/npm",
			"@semantic-release/git"
		]
	}
}
